<div class='container px-5 px-sm-0'>
  <div class='row'>
    <div class='container px-5 px-sm-0'>
      <div class='col-md-12'>
        <% if @model == 'item' %>
          <h2>検索商品"<%= @content %>"</h2>
          <%= render 'public/items/search', items: @records %>
        <% else @model == 'admin' %>
          <h2>検索メーカー"<%= @content %>"</h2>
          <%= render 'public/admins/search', admins: @records %>
        <% end %>
      </div>
    </div>
  </div>
</div>