<%= render 'public/customers/header' %>
<div class='col-md-12'>
  <% if @model == 'item' %>
    <h2>Items search for "<%= @content %>"</h2>
    <%= render 'public/items/search', items: @records %>
  <% else @model == 'admin' %>
    <h2>Admins search for "<%= @content %>"</h2>
    <%= render 'public/admins/search', admins: @records %>
  <% end %>
</div>